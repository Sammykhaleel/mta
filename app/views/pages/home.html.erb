          <tbody>
            <% Alert.all.each do |alert| %>
              <tr>
                <td><b><font size="3" color="black"><%= alert.start %></font></b>
                <td><b><font size="3" color="black"><%= alert.end %></font></b>
                <td><b><font size="3" color="black"><%= alert.sms %></font></b>
                <td><b><font size="3" color="black"><%= alert.email %></font></b>
                <td><%= link_to 'Edit', edit_alert_path(alert) %></td>
                <td><%= link_to 'Destroy', alert, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                <td><%= link_to 'Send', go_alert_path %></td>
              </tr>

            <% end %>
          </tbody>
        </table>

