    <% Service.all.each do |train| %>
            <% if train.name == "456" %>
                <div class="circle green">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>    
            <% elsif train.name == "123" %>
                <div class="circle red">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>    
            <% elsif train.name == "7" %>
                <div class="circle purple">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "ACE" %>
                <div class="circle blue">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "BDFM" %>
                <div class="circle orange">    
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "G" %>
                <div class="circle light-green">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "JZ" %>
                <div class="circle brown">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "L" %>
                <div class="circle grey">
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% elsif train.name == "NQR" %>
                <div class="circle yellow">    
                    <h4><%= train.name %></h4>
                    <p><%=  train.traffic %></p>
                </div>
            <% else train.name == "S" %>
                <div class="circle grey"> 
                    <h4><%= train.name %></h4> 
                    <p><%=  train.traffic %></p>                      
                </div>
            <% end %>
            <!-- <h4><%= train.name %></h4> -->
            <!-- <p><%=  train.traffic %></p> -->
     <% end %>    



